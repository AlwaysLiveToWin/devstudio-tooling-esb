<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.jdt.ui.classpathContainerPage">
		<classpathContainerPage id=""
			name="WSO2 Classpath Libraries"
			class="org.wso2.developerstudio.eclipse.esb.libraries.ui.wizard.WSO2LibraryWizardPage" />
	</extension>
    <extension point="org.wso2.developerstudio.libraries.libraryprovider">
		<category id="org.wso2.developerstudio.eclipse.libraries.category.axis2"
			name="Axis2">
			<description>
				Axis2 libraries
         </description>
		</category>
		<category id="org.wso2.developerstudio.eclipse.libraries.category.esb"
			name="ESB">
			<description>
				Libraries to work with ESB Server.
         </description>
		</category>
		<category id="org.wso2.developerstudio.eclipse.libraries.category.greg"
			name="Governance Registry">
			<description>
				Governance Registry libraries
         </description>
		</category>
	</extension>
	<extension point="org.wso2.developerstudio.libraries.libraryprovider">
		<library jar="lib/axiom_1.2.11.wso2v11.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-codec_1.4.0.wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-collections_3.2.2.wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-httpclient_3.1.0.wso2v3.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-io_2.4.0.wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-lang_2.6.0.wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/commons-logging-1.1.1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/geronimo-stax-api_1.0_spec_1.0.1.wso2v2.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/httpcore_4.3.3.wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/org.wso2.carbon.registry.core_4.4.7.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.greg" />
		<library jar="lib/org.wso2.carbon.user.core_4.4.7.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.greg" />
		<library jar="lib/woden_1.0.0.M9-wso2v1.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/wsdl4j_1.6.2.wso2v4.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/XmlSchema_1.4.7.wso2v3.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.axis2" />
		<library jar="lib/synapse-core_2.1.7.wso2v4-SNAPSHOT.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.esb" />
		<library jar="lib/synapse-commons_2.1.7.wso2v4-SNAPSHOT.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.esb" />
		<library jar="lib/synapse-tasks_2.1.7.wso2v4-SNAPSHOT.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.esb" />
		<library jar="lib/synapse-nhttp-transport_2.1.7.wso2v4-SNAPSHOT.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.esb" />
		<library jar="lib/org.wso2.carbon.governance.registry.extensions_4.5.2.jar"
			categoryId="org.wso2.developerstudio.eclipse.libraries.category.greg" />
	</extension>
	
	<extension point="org.eclipse.ui.startup">
		<startup class="org.wso2.developerstudio.eclipse.esb.libraries.LibraryBundleStartupHandler"/>
	</extension>

</plugin>
